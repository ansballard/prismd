{
  "name": "prismd",
  "version": "0.1.0-alpha1",
  "description": "",
  "main": "dist/prismd.builder.mjs",
  "scripts": {
    "build": "run-p build:*",
    "build:builder": "esbuild --bundle --platform=browser --format=esm  --outfile=dist/prismd.builder.mjs src/prismd.builder.mjs",
    "build:react": "esbuild --bundle --platform=browser --format=esm --external:react --minify --outfile=dist/prismd.react.mjs src/prismd.react.mjs",
    "build:preact": "esbuild --bundle --platform=browser --format=esm --external:preact --minify --outfile=dist/prismd.preact.mjs src/prismd.preact.mjs",
    "build:examples": "run-p build:examples:*",
    "build:examples:preact": "esbuild --bundle --platform=browser --jsx-factory=h --loader:.md=text --format=iife --outfile=examples/index.preact.js examples/index.preact.jsx",
    "build:examples:react": "esbuild --bundle --define:process.env.NODE_ENV='\"development\"' --platform=browser --loader:.md=text --format=iife --outfile=examples/index.react.js examples/index.react.jsx"
  },
  "files": [
    "src",
    "dist/prismd.builder.mjs",
    "dist/prismd.react.mjs",
    "dist/prismd.preact.mjs"
  ],
  "keywords": [],
  "author": "Aaron Ballard (ans.ballard@gmail.com)",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "esbuild": "^0.8.18",
    "marked": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "preact": "^10.5.7",
    "prismjs": "^1.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "snarkdown": "^2.0.0",
    "uvu": "^0.5.1"
  }
}
